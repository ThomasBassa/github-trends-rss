&lt;div class="highlight highlight-source-cs"&gt;&lt;pre&gt;&lt;span class="pl-k"&gt;public&lt;/span&gt; &lt;span class="pl-k"&gt;class&lt;/span&gt; &lt;span class="pl-en"&gt;Main&lt;/span&gt;
{
    &lt;span class="pl-c"&gt;&lt;span class="pl-c"&gt;//&lt;/span&gt; Simple Msgbox&lt;/span&gt;
    &lt;span class="pl-k"&gt;public&lt;/span&gt; &lt;span class="pl-k"&gt;static&lt;/span&gt; &lt;span class="pl-k"&gt;void&lt;/span&gt; &lt;span class="pl-en"&gt;CN&lt;/span&gt;(&lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;H&lt;/span&gt;, &lt;span class="pl-k"&gt;int&lt;/span&gt; &lt;span class="pl-smi"&gt;P&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;K&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;SP&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;PW&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;FP&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;HW&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;BT&lt;/span&gt;, &lt;span class="pl-k"&gt;string&lt;/span&gt; &lt;span class="pl-smi"&gt;PB&lt;/span&gt;)
    {
        &lt;span class="pl-en"&gt;Msgbox&lt;/span&gt;(&lt;span class="pl-s"&gt;&lt;span class="pl-pds"&gt;"&lt;/span&gt;Hello Client!&lt;span class="pl-pds"&gt;"&lt;/span&gt;&lt;/span&gt;);

        &lt;span class="pl-en"&gt;Send&lt;/span&gt;(&lt;span class="pl-s"&gt;&lt;span class="pl-pds"&gt;"&lt;/span&gt;MSG&lt;span class="pl-pds"&gt;"&lt;/span&gt;&lt;/span&gt; &lt;span class="pl-k"&gt;+&lt;/span&gt; &lt;span class="pl-smi"&gt;SPL&lt;/span&gt; &lt;span class="pl-k"&gt;+&lt;/span&gt; &lt;span class="pl-s"&gt;&lt;span class="pl-pds"&gt;"&lt;/span&gt;Hello Server!&lt;span class="pl-pds"&gt;"&lt;/span&gt;&lt;/span&gt;);
		&lt;span class="pl-c"&gt;&lt;span class="pl-c"&gt;//&lt;/span&gt; Client will send msg back to server, MSG will be showen in [LOG] Tab&lt;/span&gt;
    }
}&lt;/pre&gt;&lt;/div&gt;
